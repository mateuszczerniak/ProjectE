@model IEnumerable<ProjectE.Models.WorkReason>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "WorkReasonCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "WorkReasonEdit", new { id=item.WorkReasonId }) |
            @Html.ActionLink("Delete", "WorkReasonDelete", new { id=item.WorkReasonId })
        </td>
    </tr>
}

</table>
