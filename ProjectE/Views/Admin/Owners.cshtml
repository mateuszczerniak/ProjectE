@model IEnumerable<ProjectE.Models.Owner>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "OwnerCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "OwnerEdit", new { id=item.OwnerId }) |
            @Html.ActionLink("Delete", "OwnerDelete", new { id=item.OwnerId })
        </td>
    </tr>
}

</table>
